{
  "name": "Nina+",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#eeffff",
    "activityBarBadge.background": "#389de0",
    "sideBarTitle.foreground": "#ffffff"
  },
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": [
        "comment",
        "keyword.operator.comparison",
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#D5CED9"
      }
    },
    {
      "name": "Comment color",
      "scope": [
        "comment",
        "markup.quote.markdown",
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#5f6167"
      }
    },
    {
      "name": "Text Color",
      "scope": [
        "meta.template.expression.js",
        "constant.name.attribute.tag.jade",
        "punctuation.definition.metadata.markdown",
        "punctuation.definition.string.end.markdown",
        "punctuation.definition.string.begin.markdown",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#e8a1eb"
      }
    },
    {
      "name": "Cyan",
      "scope": [
        "support.variable",
        "entity.name.tag.yaml",
        "constant.character.entity.html",
        "source.css entity.name.tag.reference",
        "beginning.punctuation.definition.list.markdown",
        "source.css entity.other.attribute-name.parent-selector",
        "meta.structure.dictionary.json support.type.property-name",
        "variable.other.constant.object",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#fff6a3"
      }
    },
    {
      "name": "Orange",
      "scope": [
        "markup.bold",
        "meta.group.regexp",
        "constant.other.php",
        "support.constant.ext.php",
        "constant.other.class.php",
        "support.constant.core.php",
        "fenced_code.block.language",
        "constant.other.caps.python",
        "support.type.exception.python",
        "source.css keyword.other.unit",
        "variable.other.object.property.js.jsx"
      ],
      "settings": {
        "foreground": "#ffc361"
      }
    },
    {
      "name": "Yellow",
      "scope": [
        "text.xml string",
        "entity.name.type",
        "meta.at-rule.extend",
        "entity.other.inherited-class",
        "entity.other.keyframe-offset.css",
        "text.html.markdown string.quoted",
        "meta.function-call.generic.python",
        "meta.at-rule.extend support.constant",
        "source.css entity.other.attribute-name",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#b6ebc4"
      }
    },
    {
      "name": "GreenC",
      "scope": [
        "markup.heading",
        "variable.language.this.js",
        "variable.language.special.self.python",
        "support.function",
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#7dff9e"
      }
    },
    {
      "name": "RedAtri",
      "scope": [
        "entity.name.tag",
        "keyword.operator.arithmetic",
        "keyword.operator.relational",
        "keyword.control.conditional",
        "keyword.operator.ternary",
        "keyword.operator.type.annotation"
      ],
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "name": "Yelllow",
      "scope": ["constant", "support.constant.property-value"],
      "settings": {
        "foreground": "#f3e8a7"
      }
    },
    {
      "name": "SJISDJI",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "YellowStrings",
      "scope": [
        "meta.property-value",
        "support.type.property-name.json",
        "support.type.property-name.css",
        "entity.other.attribute-name",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#c6fffc"
      }
    },
    {
      "name": "Bluesx",
      "scope": [
        "punctuation.definition.interpolation",
        "punctuation.section.embedded.end.php",
        "punctuation.section.embedded.end.ruby",
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.begin.ruby",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#8f90eb"
      }
    },
    {
      "name": "Purple",
      "scope": [
        "storage",
        "keyword",
        "meta.link",
        "meta.image",
        "markup.italic",
        "source.js support.type",
        "support.class",
        "keyword.operator",
        "support.constant",
        "text.html.markdown string",
        "source.css support.function",
        "source.php support.function",
        "support.function.magic.python",
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#be87fd"
      }
    },
    {
      "name": "Blue",
      "scope": [
        "string.regexp",
        "markup.changed",
        "markup.inserted",
        "support.type.primitive",

        "entity.name.type"
      ],
      "settings": {
        "foreground": "#84cadf"
      }
    },
    {
      "name": "Font Underline",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "fontStyle": "underline"
      }
    },

    {
      "name": "Booleans, Undefined and Null",
      "scope": [
        "constant.language.null",
        "constant.language.boolean",
        "constant.language.undefined",
        "constant.numeric.decimal"
      ],
      "settings": {
        "foreground": "#f08686"
      }
    },
    {
      "name": "Components Tags",
      "scope": ["support.class.component"],
      "settings": {
        "foreground": "#4EC9B0"
      }
    },
    {
      "name": "Stringss",
      "scope": ["string"],
      "settings": {
        "foreground": "#ecb19a"
      }
    }
  ]
}
