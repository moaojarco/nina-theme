    "activityBar.foreground": "#D7DAE0",
    "activityBarBadge.background": "#528bff",
    "activityBarBadge.foreground": "#F8FAFD",
    "button.background": "#528bff",
    "diffEditor.insertedTextBackground": "#00809B33",
    "dropdown.background": "#1d1f23",
    "dropdown.border": "#181A1F",
    "editor.background": "#212121",
    "editor.findMatchBackground": "#42557B",
    "editor.findMatchHighlightBackground": "#314365",
    "editor.lineHighlightBackground": "#1e1e1e",
    "editor.selectionBackground": "#1e1e1e",
    "editorCursor.foreground": "#f8f8f0",
    "editorError.foreground": "#c24038",
    "editorGroup.emptyBackground": "#1e1e1e",
    "editorGroup.border": "#1e1e1e",
    "editorGroupHeader.tabsBackground": "#21252B",
    "editorIndentGuide.background": "#1e1e1e",
    "editorHoverWidget.background": "#21252B",
    "editorHoverWidget.border": "#181A1F",
    "editorLineNumber.foreground": "#495162",
    "editorRuler.foreground": "#484848",
    "editorSuggestWidget.background": "#21252B",
    "editorSuggestWidget.border": "#181A1F",
    "editorSuggestWidget.selectedBackground": "#2c313a",
    "editorUnnecessaryCode.opacity": "#000000c0",
    "editorWhitespace.foreground": "#484a50",
    "editorWidget.background": "#21252B",
    "input.background": "#1d1f23",
    "list.activeSelectionBackground": "#2c313a",
    "list.activeSelectionForeground": "#d7dae0",
    "list.focusBackground": "#383E4A",
    "list.highlightForeground": "#C5C5C5",
    "list.hoverBackground": "#292d35",
    "list.inactiveSelectionBackground": "#2c313a",
    "list.inactiveSelectionForeground": "#d7dae0",
    "notifications.background": "#21252b",
    "scrollbarSlider.activeBackground": "#747D9180",
    "scrollbarSlider.background": "#4E566680",
    "scrollbarSlider.hoverBackground": "#5A637580",
    "sideBar.background": "#1e1e1e",
    "sideBarSectionHeader.background": "#282c34",
    "statusBar.background": "#21252B",
    "statusBar.foreground": "#9da5b4",
    "statusBarItem.hoverBackground": "#2c313a",
    "statusBar.noFolderBackground": "#21252B",
    "statusBar.debuggingBackground": "#21252B",
    "tab.activeBackground": "#383E4A",
    "tab.border": "#181A1F",
    "tab.inactiveBackground": "#21252B",
    "terminal.foreground": "#abb2bf",
    "terminal.ansiBlack": "#2d3139",
    "terminal.ansiBlue": "#528bff",
    "terminal.ansiGreen": "#98c379",
    "terminal.ansiYellow": "#e5c07b",
    "terminal.ansiCyan": "#56b6c2",
    "terminal.ansiMagenta": "#c678dd",
    "terminal.ansiRed": "#e06c75",
    "terminal.ansiWhite": "#d7dae0",
    "terminal.ansiBrightBlack": "#7f848e",
    "terminal.ansiBrightBlue": "#528bff",
    "terminal.ansiBrightGreen": "#98c379",
    "terminal.ansiBrightYellow": "#e5c07b",
    "terminal.ansiBrightCyan": "#56b6c2",
    "terminal.ansiBrightMagenta": "#7e0097",
    "terminal.ansiBrightRed": "#f44747",
    "terminal.ansiBrightWhite": "#d7dae0",
    "titleBar.activeBackground": "#282c34",
    "titleBar.activeForeground": "#9da5b4",
    "titleBar.inactiveBackground": "#282C34",
    "titleBar.inactiveForeground": "#6B717D"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#6272A4"
      }
    },
    {
      "name": "Null",
      "scope": ["constant.language.null.js"],
      "settings":{
        "foreground":"#BD93F9"
      }
    },
    {
      "name": "Objects",
      "scope": ["support.type.property-name", "variable.object.property"],
      "settings": {
        "foreground": "#FCF7F7"
      }
    },
    {
      "name": "Variables ++",
      "scope": [
        "entity.name.tag.yaml",
        "constant.character.entity.html",
        "source.css entity.name.tag.reference",
        "source.css entity.other.attribute-name.parent-selector",
        "variable.other.readwrite",
        "variable.other.constant",
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#fff48f"
      }
    },
    {
      "name": "Orange",
      "scope": [
        "markup.bold",
        "meta.group.regexp",
        "constant.other.php",
        "support.constant.ext.php",
        "constant.other.class.php",
        "support.constant.core.php",
        "fenced_code.block.language",
        "constant.other.caps.python",
        "support.type.exception.python",
        "source.css keyword.other.unit",
        "variable.other.object.property.js.jsx",
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "orange with italic",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground":"#FFB86C",
        "fontStyle": "italic"
      }

    },

    {
      "name": "Limes",
      "scope": [
        "text.xml string",
        "entity.name.type",
        "meta.at-rule.extend",
        "entity.other.keyframe-offset.css",
        "text.html.markdown string.quoted",
        "meta.function-call.generic.python",
        "meta.at-rule.extend support.constant",
        "entity.other.attribute-name.js.jsx"
      ],
      "settings": {
        "foreground": "#b6ebc4"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "support.function",
        "entity.name.function"
      ],
      "settings":{
        "foreground":"#7dff9e"
      }
    },
    {
      "name": "Function Name",
      "scope": [
        "entity.other.attribute-name.js.jsx",
        "entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#7dff9e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tag Name / Boolean",
      "scope": [
        "entity.name.tag",
        "keyword.control.export"
      ],
      "settings": {
        "foreground": "#fa68b8"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.other.property",
        "meta.property-value",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#f2c6b3"
      }
    },
    {
      "name": "BlueP",
      "scope": [
        "punctuation.definition.interpolation",
        "punctuation.section.embedded.end.php",
        "punctuation.section.embedded.end.ruby",
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.begin.ruby",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#8f90eb"
      }
    },
    {
      "name": "Purple",
      "scope": [
        "storage",
        "keyword",
        "meta.link",
        "meta.image",
        "markup.italic",
        "source.js support.type",
        "support.class",
        "keyword.operator",
        "support.constant",
        "text.html.markdown string",
        "source.css support.function",
        "source.php support.function",
        "support.function.magic.python",
        "markup.deleted",
        "keyword.control.export"
      ],
      "settings": {
        "foreground": "#af69ff"
      }
    },
    {
      "name": "Blue",
      "scope": [
        "string.regexp",
        "markup.changed",
        "markup.inserted",
        "support.type.primitive",
        "support.class.component",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#8BE9FD",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Classes", 
      "scope": ["entity.name.type.class", "entity.other.inherited-class.js.jsx"], 
      "settings": {
      "foreground": "#8BE9FD",
      "fontStyle": "normal"
    }
    },
    {
      "name": "White",
      "scope": ["punctuation.accessor", "variable.other.object", "variable.other.property"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Numbers and Boolean",
      "scope": ["constant.numeric", "constant.language.boolean"],
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "name": "This Keyword",
      "scope": "variable.language.this.js.jsx",
      "settings": {
        "foreground": "#BD93F9",
        "fontStyle": "italic"
      }
    }
  ]
}
