{
  "name": "Nina",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#eeffff",
    "activityBarBadge.background": "#389de0",
    "sideBarTitle.foreground": "#ffffff"
  },
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": [
        "comment",
        "keyword.operator.comparison",
        "keyword.operator.assignment",
        "keyword.operator.spread",
        "keyword.operator.ternary"
      ],
      "settings": {
        "foreground": "#D5CED9"
      }
    },
    {
      "name": "Comment color",
      "scope": [
        "comment",
        "markup.quote.markdown",
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#5f6167"
      }
    },
    {
      "name": "Text Color",
      "scope": [
        "meta.template.expression.js",
        "constant.name.attribute.tag.jade",
        "punctuation.definition.metadata.markdown",
        "punctuation.definition.string.end.markdown",
        "punctuation.definition.string.begin.markdown",
        "keyword.operator.assignment",
        "storage.type.function.arrow",
        "keyword.operator.arithmetic",
        "keyword.operator.relational",
        "keyword.control.conditional",
        "keyword.operator.ternary",
        "keyword.operator.type.annotation",
        "keyword.operator.spread"
      ],
      "settings": {
        "foreground": "#D5CED9"
      }
    },
    {
      "name": "Cyan",
      "scope": [
        "support.variable",
        "entity.name.tag.yaml",
        "constant.character.entity.html",
        "source.css entity.name.tag.reference",
        "beginning.punctuation.definition.list.markdown",
        "source.css entity.other.attribute-name.parent-selector",
        "meta.structure.dictionary.json support.type.property-name",
        "variable.other.constant.object",
        "variable.other.readwrite",
        "variable.other.constant"
      ],
      "settings": {
        "foreground": "#fff6a3"
      }
    },
    {
      "name": "Orange",
      "scope": [
        "markup.bold",
        "meta.group.regexp",
        "constant.other.php",
        "support.constant.ext.php",
        "constant.other.class.php",
        "support.constant.core.php",
        "fenced_code.block.language",
        "constant.other.caps.python",
        "support.type.exception.python",
        "source.css keyword.other.unit",
        "variable.other.object.property.js.jsx"
      ],
      "settings": {
        "foreground": "#ffb86c"
      }
    },
    {
      "name": "Yellow",
      "scope": [
        "text.xml string",
        "entity.name.type",
        "meta.at-rule.extend",
        "entity.other.inherited-class",
        "entity.other.keyframe-offset.css",
        "text.html.markdown string.quoted",
        "meta.function-call.generic.python",
        "meta.at-rule.extend support.constant",
        "source.css entity.other.attribute-name",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#b6ebc4"
      }
    },
    {
      "name": "GreenC",
      "scope": [
        "markup.heading",
        "variable.language.this.js",
        "variable.language.special.self.python",
        "support.function",
        "entity.name.function",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#7dff9e"
      }
    },
    {
      "name": "RedAtri",
      "scope": [
        "entity.name.tag",
        "keyword.control.export",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#fa68b8"
      }
    },
    {
      "name": "GlossBlue",
      "scope": [
        "variable.other.property",
        "meta.property-value",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#b2cae6"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#ecb19a"
      }
    },
    {
      "name": "Hot Pink",
      "scope": [
        "punctuation.definition.interpolation",
        "punctuation.section.embedded.end.php",
        "punctuation.section.embedded.end.ruby",
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.begin.ruby",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#8f90eb"
      }
    },
    {
      "name": "Purple",
      "scope": [
        "storage",
        "keyword",
        "meta.link",
        "meta.image",
        "markup.italic",
        "source.js support.type",
        "support.class",
        "keyword.operator",
        "support.constant",
        "text.html.markdown string",
        "source.css support.function",
        "source.php support.function",
        "support.function.magic.python",
        "markup.deleted",
        "keyword.control.export"
      ],
      "settings": {
        "foreground": "#af69ff"
      }
    },
    {
      "name": "Blue",
      "scope": [
        "string.regexp",
        "markup.changed",
        "markup.inserted",
        "support.type.primitive",
        "support.class.component",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#84cadf"
      }
    },
    {
      "name": "Texts",
      "scope": [
        "text.html.php string",
        "markup.inline.raw",
        "text.html meta.embedded source.js string",
        "text.html.php punctuation.definition.string",
        "text.html meta.embedded source.js punctuation.definition.string",
        "text.html punctuation.definition.string",
        "text.html string",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#ffb86c"
      }
    },
    {
      "name": "Font Underline",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Blue Light",
      "scope": ["constant", "variable.other.constant.object"],
      "settings": {
        "foreground": "#9CDCFE"
      }
    },
    {
      "name": "White Points",
      "scope": ["punctuation.accessor", "variable.other.object", "variable.other.property"],
      "settings": {
        "foreground": "#ffffff"
      }
    }
  ]
}
